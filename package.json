{
  "name": "shufflejs",
  "version": "4.0.0",
  "description": "Categorize, sort, and filter a responsive grid of items",
  "keywords": [
    "jquery-plugin",
    "gallery",
    "shuffle",
    "layout",
    "masonry",
    "filter",
    "sort",
    "responsive",
    "grid",
    "mobile",
    "tiles"
  ],
  "main": "dist/shuffle.js",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "scriptPreprocessor": "<rootDir>/test/preprocessor.js",
    "testDirectoryName": "test",
    "testPathDirs": [
      "<rootDir>/test"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "preprocessor.js"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Vestride/Shuffle.git"
  },
  "author": "Glen Cheney <cheney.glen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vestride/Shuffle/issues"
  },
  "homepage": "https://github.com/Vestride/Shuffle",
  "dependencies": {
    "matches-selector": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "browser-sync": "^2.11.1",
    "child-process-promise": "^1.1.0",
    "gulp": "git@github.com:gulpjs/gulp.git#4.0",
    "gulp-sass": "^2.2.0",
    "gulp-util": "^3.0.7",
    "jest-cli": "^0.8.2",
    "jest-webpack-alias": "^2.2.0",
    "webpack": "^1.12.13"
  }
}
